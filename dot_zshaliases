# Misc
alias sudo="sudo "
alias c="clear"
alias rm="trash"
alias orm="/usr/bin/rm"

# VIM
alias vim="/usr/bin/nvim"
alias ovim="/usr/bin/vim"

# LSD
alias ls="lsd"
alias lso="/usr/bin/ls"
alias tree="lsd --tree"

# Projects
alias projects="cd $HOME/projects"
alias loopforge="cd $HOME/projects/loopforge"
alias organilog="cd $HOME/projects/organilog"
alias organilog:user="cd $HOME/projects/organilog/organilog_user/"

# Go Task
alias task="/usr/bin/go-task"

# Atac
alias atac="$HOME/.config/atac/launch-script.sh"
alias oatac="/usr/bin/atac"
